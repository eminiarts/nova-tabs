{
    "name": "eminiarts/nova-tabs",
    "description": "Laravel Nova - Tabs.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "authors": [
        {
            "name": "Emini Arts GmbH",
            "email": "support@eminiarts.ch",
            "homepage": "https://eminiarts.ch"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3||^8.0",
        "laravel/nova": "^3.0||^4.0",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "orchestra/testbench": "^6.7",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Eminiarts\\Tabs\\": "src/",
            "Eminiarts\\Tabs\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eminiarts\\Tabs\\TabsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "scripts": {
        "php-cs-fixer": "vendor/bin/php-cs-fixer fix",
        "phpunit": "vendor/bin/phpunit",
        "test": [
            "composer php-cs-fixer",
            "composer phpunit"
        ]
    }
}
